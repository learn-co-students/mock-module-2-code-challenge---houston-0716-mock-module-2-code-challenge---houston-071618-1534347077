<%= form_for @employee do |f|%>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
  <%= f.label :alias %>
  <%= f.text_field :alias %>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :office %>
  <%= f.text_field :office %>
  <br>
  <%= f.label :dog_id %>
  <%= f.collection_select :dog_name, Dog.all, :name, :name %>
  <br>
  <%= f.submit "Edit Employee" %>
<% end %>
