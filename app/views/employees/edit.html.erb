<h1>Edit a new employee and select 1 dog from the form list</h1>
<%= form_with model: @employee do |f| %>
<label>First Name:</label>
<%= f.text_field :first_name %><br>
<label>Last Name:</label>
<%= f.text_field :last_name %><br>
<label>Alias:</label>
<%= f.text_field :alias %><br>
<label>Title:</label>
<%= f.text_field :title %><br>
<label>Office:</label>
<%= f.text_field :office %><br>
<label>Img url:</label>
<%= f.text_field :img_url %><br>
<%= f.select :dog_id, Dog.all.map { | d | [ d.name, d.id ] } %>
<%= f.submit %>
<% end %>
