<h1>Dogs#new</h1>
<p>Find me in app/views/dogs/new.html.erb</p>


<%= form_with url: dogs_path do |form| %>
  <%= form.text_field :name, placeholder: "dog_name"%>
  <%= form.text_field :breed, placeholder: "breed"%>
  <%= form.text_field :age, placeholder: "age" %>
  <%= form.submit "Create Dog"%>


<% end %>
