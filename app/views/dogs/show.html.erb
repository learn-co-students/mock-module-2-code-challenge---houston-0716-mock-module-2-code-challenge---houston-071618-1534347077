<h1><%= @dog.name %></h1>
<ul>
  <li>Breed: <%= @dog.breed %></li>
  <li>Age: <%= @dog.age %></li>
  <li>Employees:</li>
  <ul>
    <% @dog.employees.each do |employee| %>
      <li><%= link_to employee.to_s, employee %></li>
    <% end %>
  </ul>
</ul>